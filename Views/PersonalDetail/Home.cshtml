@model IEnumerable<EmpTask.Models.Qualification>

@{
    ViewBag.Title = "Home";
}

<h2>Home</h2>

<p>
    @Html.ActionLink("Create New", "CreateQual")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mark)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Mark)
        </td>
        <td>
            @Html.ActionLink("Edit", "QualEdit", new { Id=item.Id }) |
            @*@Html.ActionLink("Details", "Details", new { id=item.Id })*@ |
            @Html.ActionLink("Delete", "DeleteQual", new { Id=item.Id })
        </td>
    </tr>
}

</table>
